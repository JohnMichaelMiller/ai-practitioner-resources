{
  "name": "ai-practitioner-resources-automation",
  "version": "1.0.0",
  "description": "Automation scripts for AI Practitioner Resources generation and gist management",
  "private": true,
  "scripts": {
    "fetch-current": "node scripts/fetch-current-resources.js",
    "generate": "node scripts/generate-resources.js",
    "merge": "node scripts/merge-and-update.js",
    "validate": "node scripts/validate-schema.js",
    "update-gist": "node scripts/update-gist.js",
    "create-summary": "node scripts/create-summary.js",
    "migrate-lanes": "node scripts/migrate-lanes-to-project-status.js",
    "intake-review": "node scripts/review-existing-issues.js",
    "pm-review-local": "node scripts/pm-review-local.js",
    "run-automation": "npm run fetch-current && npm run generate && npm run merge && npm run validate && npm run update-gist && npm run create-summary",
    "test:fetch-current": "cross-env TEST_MODE=true node scripts/fetch-current-resources.js",
    "test:generate": "cross-env TEST_MODE=true node scripts/generate-resources.js",
    "test:merge": "cross-env TEST_MODE=true node scripts/merge-and-update.js",
    "test:validate": "cross-env TEST_MODE=true node scripts/validate-schema.js",
    "test:update-gist": "cross-env TEST_MODE=true node scripts/update-gist.js",
    "test:create-summary": "cross-env TEST_MODE=true node scripts/create-summary.js",
    "test:run-automation": "npm run test:fetch-current && npm run test:generate && npm run test:merge && npm run test:validate && npm run test:update-gist && npm run test:create-summary",
    "test": "vitest run",
    "test:watch": "vitest",
    "test-lifecycle": "node scripts/test-issue-lifecycle.js",
    "test-lifecycle-dry": "cross-env DRY_RUN=true node scripts/test-issue-lifecycle.js"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "dotenv": "^17.3.1",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
